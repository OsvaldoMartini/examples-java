<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Logging Tests" parallel="false">
    <listeners>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test verbose="1" name="Test parameters">
        <parameter name="message" value="My Test message" />
        <classes>
            <class name="com.epam.reportportal.example.testng.log4j.parametrized.ParametrizedTest"/>
        </classes>
    </test>
</suite>
